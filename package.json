{
  "name": "api-hitter",
  "version": "0.0.0",
  "author": "Meeshkan",
  "bin": {
    "api-hitter": "./bin/run"
  },
  "bugs": "https://github.com/meeshkan/api-hitter/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/debug": "^4.1.5",
    "@types/js-yaml": "^3.12.1",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "loas3": "^0.1.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^24.0.18",
    "@types/node": "^10",
    "globby": "^10",
    "jest": "^24.9.0",
    "oclif": "^1.13.6",
    "ts-jest": "^24.1.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/meeshkan/api-hitter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "api-client-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "meeshkan/api-hitter",
  "scripts": {
    "start": "node bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
